		{
			"name": "POST pre request ID / Timestamp dinamicos",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code es 201\", function () {\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"\r",
							"pm.test(\"El userId coincide con el enviado\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.userId).to.eql(parseInt(pm.variables.get(\"userId\")));\r",
							"});\r",
							"\r",
							"pm.test(\"El title coincide con el enviado\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.title).to.eql(pm.variables.get(\"postTitle\"));\r",
							"});\r",
							"\r",
							""
