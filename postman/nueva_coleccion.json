				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"// Generar un userId aleatorio entre 1 y 10\r",
							"pm.variables.set(\"userId\", Math.floor(Math.random() * 10) + 1);\r",
							"\r",
							"// Crear un título dinámico con timestamp\r",
							"pm.variables.set(\"postTitle\", \"Post de prueba \" + new Date().getTime());"
